{
  "name": "toolip-programming-language",
  "displayName": "Toolip Programming Language",
  "description": "Syntaxes and snippets for the Toolip Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "tool",
        "aliases": [
          "",
          "tool"
        ],
        "extensions": [
          ".tool"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tool",
        "scopeName": "source.tool",
        "path": "./syntaxes/tool.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "coffee": "^5.5.0",
    "coffeescript": "^2.7.0"
  }
}
